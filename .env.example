# Discord Bot Token
# Get this from: https://discord.com/developers/applications
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# Google Spreadsheet ID
# Get this from your spreadsheet URL
SPREADSHEET_ID=your_spreadsheet_id_here

# Optional: Restrict bot to a specific channel
# Right-click on the channel in Discord and select "Copy Channel ID"
# (You need to enable Developer Mode in Discord settings first)
# Leave blank or remove this line to allow all channels
ALLOWED_CHANNEL_ID=
